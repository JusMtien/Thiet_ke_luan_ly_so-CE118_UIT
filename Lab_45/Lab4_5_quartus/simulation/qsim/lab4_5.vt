`timescale 1 ps/ 1 ps
module MAIN_vlg_sample_tst(
	CLK,
	N,
	START,
	sampler_tx
);
input  CLK;
input [7:0] N;
input  START;
output sampler_tx;

reg sample;
time current_time;
always @(CLK or N or START)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

