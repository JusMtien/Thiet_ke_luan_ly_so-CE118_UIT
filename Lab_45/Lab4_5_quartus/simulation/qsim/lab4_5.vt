module MAIN_vlg_vec_tst();
// constants                                           
// general purpose registers
reg CLK;
reg [7:0] N;
reg START;
// wires                                               
wire check;
wire Done;
wire [6:0] HEX0;
wire [7:0] O;
wire [7:0] Q;
wire [2:0] STATE;

wire sampler;                             

// assign statements (if any)                          
MAIN i1 (
// port map - connection between master ports and signals/registers   
	.check(check),
	.CLK(CLK),
	.Done(Done),
	.HEX0(HEX0),
	.N(N),
	.O(O),
	.Q(Q),
	.START(START),
	.STATE(STATE)
);

// CLK
always
begin
	CLK = 1'b0;
	CLK = #5000 1'b1;
	#5000;
end 

// START
initial
begin
	START = 1'b1;
	START = #20000 1'b0;
end 
// N[ 7 ]
initial
begin
	N[7] = 1'b0;
end 
// N[ 6 ]
initial
begin
	N[6] = 1'b0;
end 
// N[ 5 ]
initial
begin
	N[5] = 1'b0;
end 
// N[ 4 ]
initial
begin
	N[4] = 1'b0;
end 
// N[ 3 ]
initial
begin
	N[3] = 1'b0;
end 
// N[ 2 ]
initial
begin
	N[2] = 1'b0;
end 
// N[ 1 ]
initial
begin
	N[1] = 1'b1;
end 
// N[ 0 ]
initial
begin
	N[0] = 1'b0;
end 

MAIN_vlg_sample_tst tb_sample (
	.CLK(CLK),
	.N(N),
	.START(START),
	.sampler_tx(sampler)
);

MAIN_vlg_check_tst tb_out(
	.check(check),
	.Done(Done),
	.HEX0(HEX0),
	.O(O),
	.Q(Q),
	.STATE(STATE),
	.sampler_rx(sampler)
);
endmodule

